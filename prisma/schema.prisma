datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Skin {
  id          Int     @id @default(autoincrement())
  name        String
  price       Float
  float       Float
  wear        String
  image       String
  inspectLink String  // Novo campo adicionado para o link de inspect
}